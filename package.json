    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
